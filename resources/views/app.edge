	<script>

		let link = window.location.href;
		let pagina = link.split("/")[3]
		console.log();
		
		if(pagina === "tablero"){
				document.getElementById("navTablero").classList.add('active');
		}
		
		if(pagina === "usuariosAdministrador" || pagina === "usuariosProfesor" || pagina === "usuariosAlumno" || pagina === "usuariosExperto"){			
				document.getElementById("navUsuarios").classList.add('active');
				document.getElementById("liUsuarios").classList.add('menu-open');
			
				if(pagina === "usuariosAdministrador"){
					 document.getElementById("aAdmin").classList.add('active');
				}
			
				if(pagina === "usuariosProfesor"){
					 document.getElementById("aDocentes").classList.add('active');
				}
			
				if(pagina === "usuariosAlumno"){
					 document.getElementById("aAlumnos").classList.add('active');
				}
			
				if(pagina === "usuariosExperto"){
					 document.getElementById("aExpertos").classList.add('active');
				}
		}
		
		
		if(pagina === "configurarAccesos"){
				document.getElementById("navAccesos").className += " active";
		}
		
		
		if(pagina === "configurarCuestionario" || pagina === "cuestionario"){
			  document.getElementById("navCuestionario").classList.add('active');
				document.getElementById("liCuestionario").classList.add('menu-open');
			
				if( pagina === "configurarCuestionario"){
					 document.getElementById("aConfigurar").classList.add('active');
				}
			
				if( pagina === "cuestionario"){
					 document.getElementById("aVer").classList.add('active');
				}
				
		}
		
		if(pagina === "temas"){
			  document.getElementById("navTemas").className += " active";
		}
		
		if(pagina === "preguntasAbiertas" || pagina === "preguntasNumericas" || pagina === "preguntasVerdaderoFalso" || pagina === "preguntasOpcionMultiple" || pagina === "preguntasCalculadas" || pagina === "PreguntasOpcionMultipleCalculadas"){
			  
				document.getElementById("navPreguntas").classList.add('active');
				document.getElementById("liPreguntas").classList.add('menu-open');
			
				if( pagina === "preguntasAbiertas" ){
					 document.getElementById("aAbiertas").classList.add('active');
				}
			
				if( pagina === "preguntasNumericas" ){
					 document.getElementById("aNumericas").classList.add('active');
				}
			
				if( pagina === "preguntasVerdaderoFalso" ){
					 document.getElementById("aVerdaderoFalso").classList.add('active');
				}
			
				if( pagina === "preguntasOpcionMultiple" ){
					 document.getElementById("aMultiple").classList.add('active');
				}
			
				if( pagina === "preguntasCalculadas" ){
					 document.getElementById("aCalculadas").classList.add('active');
				}
			
				if( pagina === "PreguntasOpcionMultipleCalculadas" ){
					 document.getElementById("aMCalculadas").classList.add('active');
				}
		}
		
		
		if(pagina === "subirImagen"){
			  document.getElementById("navRecursos").classList.add('active');
		}
		
		
		
		window.idusuario = {{ auth.user.id }}
	</script>
	

